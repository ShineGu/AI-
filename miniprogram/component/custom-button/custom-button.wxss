/* components/custom-button/custom-button.wxss */
.custom-button {
  width: 240rpx;
  height: 15vh;
}

.custom-button .button {
  position: relative;
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
  /* background-image: "/images/test.png"; */
}

.button .image {
  position: absolute;
  top: 1rpx;
  left: 1rpx;
  width: 98%;
  height: 98%;
}

@font-face {
  font-family: 'HYXingKaiF';
  src: url('data:font/ttf;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

.button .text {
  position: relative;
  top: 130rpx;
  font-family: "HYXingKaiF";
  font-size: 60rpx;
  color: black;
  font-weight: bold;
}